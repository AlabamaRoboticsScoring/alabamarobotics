<p id="notice"><%= notice %></p>

<h1>Listing Attempts</h1>



<table class= "table table-striped">
  <thead>
    <tr>
      <th>Team</th>
      <th>Course</th>
      <th>Points</th>
      <th>Completion time</th>
      <th>Judge name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attempts.each do |attempt| %>
      <tr>
        <td><%= attempt.team %></td>
        <td><%= attempt.course %></td>
        <td><%= attempt.points %></td>
        <td><%= attempt.completion_time %></td>
        <td><%= attempt.judge_name %></td>
        <td><%= link_to 'Show', attempt %></td>
        <td><%= link_to 'Edit', edit_attempt_path(attempt) %></td>
        <td><%= link_to 'Destroy', attempt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Attempt', new_attempt_path %>
